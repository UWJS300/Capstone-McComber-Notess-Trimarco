{
  "manifest_version": 2,

  "name": "Mindful Reader Chrome Extension",
  "description": "A browser extension that allows you to track your mood while you read the news",
  "version": "1.0",
  "permissions": [
  "activeTab"
],

"background": {
  "scripts": ["event.js"],
  "persistent": false
},

  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "index.html"
  },

  "icons": { "24": "icon24.png",
           "48": "icon48.png"

  }
}
